{
    "openapi": "3.0.1",
    "info": {
        "title": "envSensor",
        "description": "Describes Environment sensor API's",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "https://{serverIP}:{serverPort}/api/1.0.0/resource",
            "description": "Resource server",
            "variables": {
                "serverIP": {
                    "default": "pune.iudx.org.in"
                },
                "serverPort": {
                    "default": "8080"
                }
            }
        }
    ],
    "paths": {
        "/latest/aqm-bosch-climo/{NAME}": {
            "get": {
                "description": "Get the sensor message as an array between given time points",
                "operationId": "latest",
                "responses": {
                    "200": {
                        "description": "Sensor Message as JSON",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SensorPacket"
                                }
                            }
                        }
                    }
                }
            },
            "parameters": [
                {
                    "name": "NAME",
                    "in": "path",
                    "description": "Name of the location where sensor is deployed",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "name": "id",
                    "in": "header",
                    "description": "User id",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "name": "apikey",
                    "in": "header",
                    "description": "API Key",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        },
        "/query/aqm-bosch-climo/{NAME}/{startTime}/{endTime}": {
            "get": {
                "description": "Get the sensor message as an array between given time points",
                "operationId": "query",
                "responses": {
                    "200": {
                        "description": "Sensor Message as JSON",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SensorPacketArray"
                                }
                            }
                        }
                    }
                }
            },
            "parameters": [
                {
                    "name": "NAME",
                    "in": "path",
                    "description": "Name of the location where sensor is deployed",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "name": "id",
                    "in": "header",
                    "description": "User id",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "name": "apikey",
                    "in": "header",
                    "description": "API Key",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "name": "startTime",
                    "in": "path",
                    "description": "start time",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "name": "endTime",
                    "in": "path",
                    "description": "End time",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    },
