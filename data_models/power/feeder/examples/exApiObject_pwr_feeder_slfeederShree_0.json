{
    "openapi": "3.0.1",
    "info": {
        "title": "StreetLightingSreePanel",
        "description": "The data model for Shree Streelighting feeder station",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "https://{serverIP}:{serverPort}/api/1.0.0/resource",
            "description": "",
            "variables": {
                "serverIP": {
                    "default": "localhost"
                },
                "serverPort": {
                    "default": "8080"
                }
            }
        }
    ],
    "paths": {
        "/latest/streetlight-feeder-sree/{NAME}/": {
            "get": {
                "description": "Get feeder station data",
                "operationId": "latest",
                "parameters": [
                    {
                        "name": "NAME",
                        "in": "path",
                        "description": "Name of the nearest streetlight feeder station",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SensorPacket"
                                }
                            }
                        }
                    }
                }
            },
            "parameters": [
                {
                    "name": "NAME",
                    "in": "path",
                    "description": "Feeder location name",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "name": "id",
                    "in": "header",
                    "description": "User ID",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "name": "apikey",
                    "in": "header",
                    "description": "API key for that user",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        },
        "/query/streetlight-feeder-sree/{NAME}/{startTime}/{endTime}/": {
            "get": {
                "description": "Get feeder station data",
                "operationId": "query",
                "parameters": [
                    {
                        "name": "NAME",
                        "in": "path",
                        "description": "Name of the nearest streetlight feeder station",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "startTime",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "endTime",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SensorPacketArray"
                                }
                            }
                        }
                    }
                }
            },
            "parameters": [
                {
                    "name": "NAME",
                    "in": "path",
                    "description": "Feeder location name",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "name": "id",
                    "in": "header",
                    "description": "User ID",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "name": "apikey",
                    "in": "header",
                    "description": "API key for that user",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    },
    "components": {
        "schemas": {
            "SensorPacket": {
                "description": "Wrapper object for the sensor message",
                "required": [
                    "StreetLightingSreePanel"
                ],
                "type": "object",
                "properties": {
                    "StreetLightingSreePanel": {
                        "$ref": "#/components/schemas/StreetLightingSreePanel"
                    }
                }
            },
